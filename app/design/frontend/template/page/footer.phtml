<!-- <!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Myntra Style Footer</title>
    <style>
        /* Reset Styles */
        .myntra-footer-reset {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: 'Assistant', 'Roboto', sans-serif;
        }

        /* Body Styling */
        .myntra-footer-body-container {
            min-height: 100vh;
            padding-bottom: 300px;
            position: relative;
        }

        /* Footer Main Container */
        .myntra-footer-main-container {
            background-color: #FAFBFC;
            border-top: 1px solid #eaeaec;
            color: #696b79;
            position: absolute;
            bottom: 0;
            width: 100%;
        }

        /* Footer Content Wrapper */
        .myntra-footer-content-wrapper {
            max-width: 1200px;
            margin: 0 auto;
            padding: 15px 20px;
        }

        /* Footer Top Section */
        .myntra-footer-top-section {
            display: flex;
            flex-wrap: wrap;
            justify-content: space-between;
            margin-bottom: 20px;
        }

        /* Footer Column */
        .myntra-footer-column {
            width: 20%;
            min-width: 180px;
            margin-bottom: 10px;
        }

        /* Footer Column Heading */
        .myntra-footer-column-heading {
            color: #282c3f;
            font-size: 14px;
            font-weight: 600;
            margin-bottom: 12px;
            text-transform: uppercase;
        }

        /* Footer Column Links List */
        .myntra-footer-column-links-list {
            list-style-type: none;
            padding-left: 0;
        }

        /* Footer Column Link Item */
        .myntra-footer-column-link-item {
            margin-bottom: 8px;
        }

        /* Footer Column Link */
        .myntra-footer-column-link {
            color: #696b79;
            font-size: 13px;
            text-decoration: none;
            transition: all 0.2s ease;
        }

        .myntra-footer-column-link:hover {
            color: #ff3f6c;
            text-decoration: underline;
        }

        /* Footer Middle Section */
        .myntra-footer-middle-section {
            display: flex;
            flex-wrap: wrap;
            justify-content: space-between;
            align-items: center;
            padding: 15px 0;
            border-top: 1px solid #eaeaec;
            border-bottom: 1px solid #eaeaec;
        }

        /* Footer Popular Searches */
        .myntra-footer-popular-searches {
            width: 100%;
        }

        /* Popular Searches Heading */
        .myntra-footer-popular-searches-heading {
            color: #282c3f;
            font-size: 14px;
            font-weight: 600;
            margin-bottom: 10px;
        }

        /* Popular Searches Links */
        .myntra-footer-popular-searches-links {
            color: #696b79;
            font-size: 12px;
            line-height: 1.6;
        }

        /* Popular Searches Link */
        .myntra-footer-popular-searches-link {
            color: #696b79;
            text-decoration: none;
            margin-right: 5px;
            transition: all 0.2s ease;
        }

        .myntra-footer-popular-searches-link:hover {
            color: #ff3f6c;
            text-decoration: underline;
        }

        /* Popular Searches Separator */
        .myntra-footer-popular-searches-separator {
            margin: 0 3px;
            color: #696b79;
        }

        /* Footer Bottom Section */
        .myntra-footer-bottom-section {
            display: flex;
            flex-wrap: wrap;
            justify-content: space-between;
            align-items: center;
            padding-top: 15px;
        }

        /* Footer Contact Info */
        .myntra-footer-contact-info {
            display: flex;
            flex-wrap: wrap;
            width: 60%;
        }

        /* Contact Info Block */
        .myntra-footer-contact-info-block {
            margin-right: 30px;
            margin-bottom: 15px;
        }

        /* Contact Info Heading */
        .myntra-footer-contact-info-heading {
            color: #282c3f;
            font-size: 14px;
            font-weight: 600;
            margin-bottom: 6px;
        }

        /* Contact Info Text */
        .myntra-footer-contact-info-text {
            color: #696b79;
            font-size: 13px;
            line-height: 1.4;
            margin: 0;
        }

        /* Footer Social Media */
        .myntra-footer-social-media {
            display: flex;
            flex-direction: column;
            width: 40%;
            align-items: flex-end;
        }

        /* Social Media Heading */
        .myntra-footer-social-media-heading {
            color: #282c3f;
            font-size: 14px;
            font-weight: 600;
            margin-bottom: 8px;
        }

        /* Social Media Icons Container */
        .myntra-footer-social-media-icons {
            display: flex;
        }

        /* Social Media Icon Link */
        .myntra-footer-social-media-icon-link {
            margin-left: 15px;
            color: #696b79;
            text-decoration: none;
            transition: all 0.2s ease;
        }

        .myntra-footer-social-media-icon-link:hover {
            color: #ff3f6c;
            text-decoration: underline;
        }

        /* Footer Copyright */
        .myntra-footer-copyright {
            width: 100%;
            text-align: center;
            margin-top: 20px;
            font-size: 12px;
            color: #94969f;
        }

        /* Scroll to Top Button */
        .myntra-footer-scroll-to-top-button {
            position: fixed;
            bottom: 20px;
            right: 20px;
            width: 36px;
            height: 36px;
            background-color: #ff3f6c;
            color: white;
            border-radius: 50%;
            display: flex;
            justify-content: center;
            align-items: center;
            cursor: pointer;
            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2);
            z-index: 100;
            opacity: 0;
            visibility: hidden;
            transform: translateY(20px);
            transition: opacity 0.3s ease, visibility 0.3s ease, transform 0.3s ease, background-color 0.2s ease;
        }

        .myntra-footer-scroll-to-top-button.show {
            opacity: 1;
            visibility: visible;
            transform: translateY(0);
        }

        .myntra-footer-scroll-to-top-button:hover {
            background-color: #e62e5c;
        }

        /* Scroll to Top Button Arrow Icon */
        .myntra-footer-scroll-to-top-arrow {
            font-size: 18px;
            font-weight: bold;
            line-height: 1;
        }

        /* Responsive Styles */
        @media (max-width: 768px) {
            .myntra-footer-column {
                width: 50%;
            }
            
            .myntra-footer-contact-info, .myntra-footer-social-media {
                width: 100%;
            }
            
            .myntra-footer-social-media {
                align-items: flex-start;
                margin-top: 15px;
            }
            
            .myntra-footer-social-media-icon-link {
                margin-left: 0;
                margin-right: 15px;
            }
        }
    </style>
</head>
